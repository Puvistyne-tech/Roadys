model User {
  id        String  @id @default(cuid())
  email     String @unique
  pseudo    String @unique
  password  String
  firstname String?
  lastname  String?
  latitude  Float?
  longitude Float?
  age Int?
  description String?
  nationality String?
  kindOfTrip String?
  isVisibled Boolean?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}